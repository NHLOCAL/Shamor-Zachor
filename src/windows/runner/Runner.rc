            VALUE "InternalName", "ShamorVezachor" "\0"
            VALUE "LegalCopyright", "Copyright (C) 2025 ze-kal.top. All rights reserved." "\0"
            VALUE "OriginalFilename", "ShamorVezachor.exe" "\0"
            VALUE "ProductName", "שמור וזכור" "\0"
            VALUE "ProductVersion", VERSION_AS_STRING "\0"
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x0409, 1200 // English (US), Unicode (UTF-8)
    END
END
#endif
